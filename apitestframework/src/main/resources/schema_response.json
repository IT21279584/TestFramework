{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "response": {
      "type": "object",
      "properties": {
        "body": {
          "type": ["object", "null"],
          "properties": {
            "id": {
              "oneOf": [
                {
                  "type": "integer",
                  "minimum": 1,
                  "maximum": 70
                },
                {
                  "type": "string",
                  "pattern": "^(?:[1-9]|[1-6][0-9]|70)$"
                }
              ]
            }
          }
        }
      }
    }
  },
  "additionalProperties": false
}
